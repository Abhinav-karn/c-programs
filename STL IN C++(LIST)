//list in STL
#include <iostream>
#include <list>
using namespace std;

int main()
{
    list <int> l;
    
    l.push_back(1);
    l.push_front(2);
    l.push_front(4);
    for(int i:l){
        cout<<i<<endl;
    }

    cout<<"front "<<l.front()<<endl;
    cout<<"back "<<l.back()<<endl;
    
    cout<<"empty or not "<<l.empty()<<endl;
    
    cout<<"before erase "<<l.size()<<endl;
    l.erase(l.begin());
    cout<<"after erase "<<l.size()<<endl;
    

    return 0;
}
