#include <iostream>
#include <map>

using namespace std;

int main()
{   
    map<int,string> m;
    
    m[1]="abhinav";
    m[3]="karn";
    m[6]="mohali";
    
    m.insert({5,"cu"});
    
    cout<<"before erase "<<endl;
    for(auto i:m){
        cout<<i.first<<" "<<i.second<<endl;
    }cout<<endl<<endl;
    
    cout<<"finding 6-> "<<m.count(6)<<endl;
    
    m.erase(3);
    
    cout<<"after erase "<<endl;
    for(auto i:m){
        cout<<i.first<<" "<<i.second<<endl;
    }cout<<endl<<endl;
    
    auto it = m.find(5);
    for(auto i=it;i!=m.end();i++){
        cout<<(*i).first<<endl;
    }
    return 0;
}
