#include <iostream>

using namespace std;

int main()
{
    int a=6;
    int b=4;
    
    cout<<"a&b"<<(a&b)<<endl;
    cout<<"a|b"<<(a|b)<<endl;
    cout<<"~a"<<(~a)<<endl;
    cout<<"a^b"<<(a^b)<<endl;
    
    cout<<(15>>2)<<endl;//3
    cout<<(14>>1)<<endl;//7
    cout<<(14<<1)<<endl;//28
    cout<<(15<<2)<<endl;//60
    
    int i= 8;
    cout<<(i++)<<endl;//8 ,i=9
    cout<<(++i)<<endl;//10, i=10
    cout<<(i--)<<endl;//10, i=9
    cout<<(--i)<<endl;//8, i=8
}
